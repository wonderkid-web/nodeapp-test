generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}

model Buku {
  id         Int      @unique @default(autoincrement())
  judul_buku String
  createdAt  DateTime @default(now())
}

generator client {
  provider = "prisma-client-js"
}

